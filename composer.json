{
    "name": "tgeorgel/objectpress",
    "description": "It's time to treat wordpress like an object.",
    "type": "library",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Thomas GEORGEL",
            "email": "thomas@hydrat.agency"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "OP\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tgeorgel/wp-eloquent.git"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/config": "^9.52 || ^10.0",
        "illuminate/contracts": "^9.52 || ^10.0",
        "illuminate/support": "^9.52 || ^10.0",
        "illuminate/translation": "^9.52 || ^10.0",
        "illuminate/filesystem": "^9.52 || ^10.0",
        "illuminate/validation": "^9.52 || ^10.0",
        "illuminate/http": "^9.52 || ^10.0",
        "illuminate/database": "^9.52 || ^10.0",
        "illuminate/container": "^9.52 || ^10.0",
        "illuminate/view": "^9.52 || ^10.0",
        "illuminate/routing": "^9.52 || ^10.0",
        "illuminate/events": "^9.52 || ^10.0",
        "illuminate/pagination": "^9.52 || ^10.0",
        "as247/wp-eloquent": "^2.1",
        "phpfastcache/phpfastcache": "^9.0",
        "amphibee/wordpress-eloquent-models": "^1.2",
        "amphibee/hookable": "^1.0",
        "symfony/error-handler": "^6.0",
        "symfony/var-dumper": "^5.0|^6.0",
        "nesbot/carbon": "^2.45",
        "doctrine/inflector": "^2.0",
        "thunderer/shortcode": "^0.7.3",
        "laravel/helpers": " ^1.0",
        "guzzlehttp/guzzle": "^7.3",
        "league/flysystem": "^3.0"
    },
    "suggest": {
        "wp-grogu/acf-manager": "A powerful Object-Oriented library helping you create ACF field groups & gutemberg blocks while adding type-cast in returned values from database."
    }
}
